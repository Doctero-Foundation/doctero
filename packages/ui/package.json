{
  "name": "@doctero/ui",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "check": "biome check .",
    "check:fix": "biome check --fix --unsafe .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@doctero/ts-config": "workspace:*",
    "@tailwindcss/postcss": "^4.0.14",
    "@types/node": "20.17.24",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "postcss": "^8.5.3",
    "tailwindcss": "4.0.14",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.482.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwind-merge": "3.0.2",
    "tailwindcss-animate": "1.0.7"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./globals.css": {
      "import": "./src/styles/globals.css",
      "require": "./src/styles/globals.css"
    },
    "./postcss.config": "./postcss.config.mjs",
    "./lib/*": "./src/lib/*.ts",
    "./components/*": "./src/components/*.tsx",
    "./hooks/*": "./src/hooks/*.ts",
    "./styles/*": "./src/styles/*"
  }
}
