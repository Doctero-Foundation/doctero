{
  "extends": "@doctero/ts-config/react.json",
  "compilerOptions": {
    "types": ["vite/client"],

    "baseUrl": ".",
    "paths": {
      "@/*": ["app/*"],
      "@doctero/ui/*": ["../../packages/ui/src/*"]
    },

    "noUncheckedSideEffectImports": true
  },
  "include": ["app", "**/*.ts", "**/*.tsx"],
  "exclude": ["dist", "build", "node_modules"]
}
